{
  "name": "domb",
  "version": "0.1.0",
  "description": "Simple UI library to build reactive DOM nodes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "cd example && npm install --no-optional",
    "build": "cd lib && tsc",
    "test": "jest --watch",
    "start": "cd example && webpack-dev-server --open"
  },
  "devDependencies": {
    "typescript": "3.9.7",
    "@types/jest": "^26.0.16",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "html-webpack-plugin": "^4.5.0",
    "ts-loader": "^8.0.11",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danilofes/domb.js.git"
  },
  "author": "Danilo F. Silva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danilofes/domb.js/issues"
  },
  "homepage": "https://github.com/danilofes/domb.js#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/lib/src/**/*.test.ts"
    ]
  }
}
